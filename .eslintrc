{
    "env": {
        "browser": true,
        "es2017": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        //"ecmaVersion": 2017,
        "sourceType": "module"
    },
    "globals": {
        "utils": "readonly",
        "jQuery": "readonly", // writable
        "$": "readonly"
    },
    "rules": {
        //"no-undef": ["off"],
        "semi": ["error", "always", {"omitLastInOneLineBlock": true}]
    }
}
